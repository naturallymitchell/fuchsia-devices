executable("tee-test") {
  configs += [ "//build/unification/config:zircon-migrated" ]
  sources = [ "tee-test.c" ]
  deps = [
    "//sdk/lib/fdio",
    "//zircon/system/ulib/tee-client-api",
  ]
  libs = [ "zircon" ]

  # TODO(fxbug.dev/58162): delete the below and fix compiler warnings
  configs += [ "//build/config:Wno-conversion" ]
}
