group("bin") {
  deps = [
    "acpidump",
    "driver-index",
    "driver-registrar",
    "driver_host",
    "driver_host2",
    "driver_manager",
    "driver_runtime",
    "driverctl",
    "lsdev",
    "register-util",
    "tee-test",
    "unbind",
  ]
}

group("tests") {
  testonly = true
  deps = [
    "driver-index:tests",
    "driver_host:tests",
    "driver_host2:tests",
    "driver_manager:tests",
    "register-util:tests",
  ]
}
